<%= form_for @project do |f| %>
  <%= f.text_field :title, :autofocus => true %>
  <%= f.label :title %>
  <br>
  <%= f.text_field :code_repository_link %>
  <%= f.label :code %>
  <br>
  <%= f.text_field :explanation %>
  <%= f.label :explanation %>
  <p><p>
  <%= f.submit  class: "btn btn-primary" %>
<% end %>
